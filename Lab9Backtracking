#include<stdio.h>
#include<stdlib.h>

int st[20];
int n;

void afisare()
{
    for(int i=1;i<=n;++i)
        printf("%d ",st[i]);
        printf("\n");
}

int valid(int k)
{
    int copie=k;
    --k;
    while(k>0)
    {
        if(st[k]==st[copie])
            return 0;
        --k;
    }
    return 1;
}

int solutie(int k)
{
    if(k==n)
        return 1;
    else
        return 0;
}

void back(int k)
{
    for(int i=1;i<=n;++i)
    {
        st[k]=i;
        if(valid(k))
        {
            if(solutie(k))
                afisare();
            else
                back(k+1);
        }
    }
}

int main()
{
    printf("n= ");
    scanf("%d",&n);
    back(1);
    return 0;

}
